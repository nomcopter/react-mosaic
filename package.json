{
  "name": "react-mosaic-component",
  "version": "0.4.5",
  "description": "A React Tiling Window Manager",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "style": "lib/react-mosaic.css",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/palantir/react-mosaic.git"
  },
  "scripts": {
    "build": "npm-run-all clean build:ts build:less",
    "build:ts": "tsc -p tsconfig-build.json",
    "build:less": " lessc --source-map styles/index.less react-mosaic-component.css",
    "clean": "rm -rf lib/",
    "docs": "rm -rf docs/ && webpack",
    "start": "webpack-dev-server --config webpack.config.hot.js --inline --hot --content-base docs/ --host 0.0.0.0 --port 8092",
    "test": "npm-run-all test:lint test:unit",
    "test:circle": "npm-run-all test:lint test:unit:ci",
    "test:lint": "tslint --project tsconfig.json",
    "test:unit": "mocha --opts test/mocha.opts 'test/*.ts'",
    "test:unit:ci": "mocha --opts test/mocha.opts --reporter mocha-junit-reporter 'test/*.ts'",
    "version": "npm run docs && git add -A docs/"
  },
  "devDependencies": {
    "@blueprintjs/core": "^1.29.0",
    "@types/assertion-error": "^1.0.30",
    "@types/chai": "^4.0.4",
    "@types/classnames": "^2.2.3",
    "@types/es6-promise": "^0.0.33",
    "@types/immutability-helper": "^2.0.15",
    "@types/lodash": "^4.14.76",
    "@types/mocha": "^2.2.43",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.7",
    "@types/react-dnd": "^2.0.34",
    "@types/react-dnd-html5-backend": "^2.1.8",
    "@types/react-dom": "^15.5.4",
    "@types/react-hot-loader": "^3.0.4",
    "@types/uuid": "^3.4.2",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "file-loader": "^0.11.2",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "jsdom": "^11.2.0",
    "jsdom-global": "^3.0.2",
    "less": "^2.6.0",
    "less-loader": "^2.2.3",
    "mocha": "^3.5.3",
    "mocha-junit-reporter": "^1.12.1",
    "npm-run-all": "^4.1.1",
    "react": "^16.0.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^16.0.0",
    "react-hot-loader": "3.0.0-beta.7",
    "replace": "v0.3.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "ts-loader": "^2.3.7",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "typescript": "^2.5.2",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "immutability-helper": "^2.4.0",
    "lodash": "^4.17.2",
    "prop-types": "^15.6.0",
    "react-dnd": "^2.5.1",
    "react-dnd-html5-backend": "^2.5.1",
    "uuid": "^3.0.1"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0"
  },
  "keywords": [
    "ui",
    "palantir",
    "react",
    "component",
    "typescript",
    "tiling-window-manager",
    "window-manager"
  ]
}
